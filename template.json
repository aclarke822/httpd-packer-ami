{
    "variables": {},
    "builders": [
        {
            "communicator": "ssh",
            "global_id": "207accd",
            "_source_path": "stakahashi/amazonlinux2",
            "provider": "virtualbox",
            "add_force": true,
            "type": "vagrant"
        }
    ],
    "provisioners": [
        {
            "type": "shell",
            "script": "./setup-scripts/setup.sh"
        },
        {
          "type": "ansible-local",
          "playbook_file": "./ansible/main.yml",
          "playbook_dir": "./ansible/",
          "role_paths": "./ansible/roles",
          "extra_arguments": [
            "-vvv"
          ]
        }
    ]
}
